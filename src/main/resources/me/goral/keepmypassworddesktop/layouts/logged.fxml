<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.goral.keepmypassworddesktop.controllers.LoggedController">
   <children>
      <TableView fx:id="contentTable" layoutX="14.0" layoutY="156.0" prefHeight="287.0" prefWidth="465.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="0" text="id" />
            <TableColumn fx:id="descColumn" prefWidth="155.0" text="Description" />
            <TableColumn fx:id="loginColumn" prefWidth="155.0" text="Login" />
            <TableColumn fx:id="pwdColumn" prefWidth="155.0" text="Password" />
            <TableColumn fx:id="ivColumn" prefWidth="0" text="iv" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="509.0" layoutY="205.0" mnemonicParsing="false" onAction="#onAddClick" prefHeight="25.0" prefWidth="195" text="Add" wrapText="true" />
      <Button fx:id="removeButton" layoutX="509.0" layoutY="254.0" mnemonicParsing="false" onAction="#onRemoveClick" prefHeight="25.0" prefWidth="195.0" text="Remove" wrapText="true" />
      <Button fx:id="clearDataButton" layoutX="509.0" layoutY="304.0" mnemonicParsing="false" onAction="#onDeleteDataClick" prefHeight="25.0" prefWidth="195.0" text="Delete Data" wrapText="true" />
      <Button fx:id="deleteAccountButton" layoutX="509.0" layoutY="404.0" mnemonicParsing="false" onAction="#onDeleteAccountClick" prefHeight="25.0" prefWidth="195.0" text="Delete Account" wrapText="true" />
      <Label fx:id="titleLabel" layoutY="20.0" prefHeight="17.0" prefWidth="750.0" text="Keep My Password" />
      <Label layoutY="85.0" prefHeight="17.0" prefWidth="187.5" text="Logged as:" />
      <Label fx:id="unameLabel" layoutX="187.5" layoutY="85.0" prefHeight="17.0" prefWidth="187.5" />
      <Button fx:id="logoutButton" layoutX="509.0" layoutY="354.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" prefHeight="25.0" prefWidth="195.0" text="Logout" wrapText="true" />
      <Button fx:id="genPwd" layoutX="509.0" layoutY="155.0" mnemonicParsing="false" onAction="#onGenPwdClick" prefHeight="25.0" prefWidth="195.0" text="Generate Password" wrapText="true" />
      <Button fx:id="showBtn" layoutX="442.0" layoutY="109.0" maxWidth="16" mnemonicParsing="false" onAction="#onShowBtnClick" prefHeight="16" prefWidth="16" />
   </children>
</AnchorPane>
