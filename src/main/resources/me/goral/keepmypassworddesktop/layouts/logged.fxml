<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.goral.keepmypassworddesktop.controllers.LoggedController">
   <children>
      <TableView fx:id="contentTable" layoutX="25.0" layoutY="150.0" prefHeight="287.0" prefWidth="463.0">
        <columns>
          <TableColumn fx:id="descColumn" prefWidth="75.0" text="Description" />
          <TableColumn fx:id="loginColumn" prefWidth="75.0" text="Login" />
            <TableColumn fx:id="pwdColumn" prefWidth="75.0" text="Password" />
        </columns>
      </TableView>
      <Button fx:id="AddQuery" layoutX="503.0" layoutY="169.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="163.0" text="Add" />
      <Button fx:id="RemoveQuery" layoutX="503.0" layoutY="219.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="163.0" text="Remove" />
      <Button fx:id="DelDataButton" layoutX="503.0" layoutY="269.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="163.0" text="Delete Data" />
      <Button fx:id="delAccButton" layoutX="503.0" layoutY="369.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="163.0" text="Delete Account" />
      <Label layoutY="20.0" prefHeight="17.0" prefWidth="700.0" text="Keep My Password" />
      <Label layoutY="70.0" prefHeight="17.0" prefWidth="350.0" text="Logged as:" />
      <Label fx:id="unameLabel" layoutX="350.0" layoutY="70.0" prefHeight="17.0" prefWidth="350.0" />
      <Button fx:id="logoutButton" layoutX="503.0" layoutY="319.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="163.0" text="Logout" onAction="#onLogoutButtonClick"/>
   </children>
</AnchorPane>
